#include <stdio.h>
#include <string.h>
#include <stdlib.h>

#define MAX 10

typedef struct{
    int elem[MAX];
    int count;
} Etype, *EPtr;

EPtr L;

void initList(EPtr  L);
void insertPos(EPtr L, int data, int pos);
